{
  "name": "taskhighlights",
  "version": "0.1.0",
  "private": true,
  "main": "main/index.js",
  "productName": "Task Highlights",
  "scripts": {
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "devMigrate": "yarn prisma migrate dev",
    "prodMigrate": "yarn prisma migrate deploy",
    "introspect": "yarn prisma introspect",
    "generate": "yarn prisma generate",
    "flush": "yarn devMigrate && yarn introspect && yarn generate",
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "type-check": "tsc"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.1",
    "@prisma/client": "^2.27.0",
    "app-root-path": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-next": "^3.1.5",
    "firebase": "^8.6.8",
    "next": "11.0.0",
    "node-fetch": "^2.6.1",
    "prisma": "^2.27.0",
    "random-avatar-generator": "^2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loading-skeleton": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.2.6",
    "bundle": "^2.1.0",
    "electron": "^13.1.7",
    "eslint": "^7.30.0",
    "eslint-config-next": "11.0.0",
    "next-sitemap": "^1.6.133",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  }
}
